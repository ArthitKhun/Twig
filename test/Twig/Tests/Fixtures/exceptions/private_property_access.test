--TEST--
Twig extension block private property accesses
--TEMPLATE--
{{ object.foo }}
--DATA--
class TestClassForPrivatePropertyAccess implements \ArrayAccess
{
    public $prop = 'foo';
    private $data = array();
    private $foo = 'test';

    public function offsetExists($offset)
    {
        return array_key_exists($offset, $this->data);
    }

    public function offsetGet($offset)
    {
        return $this->data[$offset];
    }

    public function offsetSet($offset, $value)
    {
    }

    public function offsetUnset($offset)
    {
    }
}
return array('object' => new TestClassForPrivatePropertyAccess())
--EXPECT--
Twig_Error: Twig_Error_Runtime: Method "foo" for object "TestClassForPrivatePropertyAccess" does not exist in "exceptions/private_property_access.test"
